{
    "name": "sasa-b/apns2",
    "description": "APNS - Apple Push Notification Service PHP client",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "apns",
        "apns2",
        "push-notifications",
        "apple",
        "ios",
        "client",
        "http",
        "http2"
    ],
    "authors": [
        {
            "name": "Sasa Blagojevic",
            "email": "sasa.blagojevic@mail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.0",
        "guzzlehttp/guzzle": "^6.5",
        "ext-json": "*",
        "lcobucci/jwt": "^4.0",
        "ramsey/uuid": "^4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "SasaB\\Apns\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SasaB\\Apns\\Tests\\": "tests/"
        }
    }
}
